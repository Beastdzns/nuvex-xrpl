#-------------------------------------- CONTRACT DEPLOYMENT ----------------------------------
# Copy this to .env and fill in your values

# Private key for deployment (with 0x prefix)
PRIVATE_KEY=your_private_key_here

# RPC URLs for different networks
MAINNET_RPC_URL=https://mainnet.infura.io/v3/your_infura_key
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/your_infura_key

# Etherscan API keys for verification
ETHERSCAN_API_KEY=your_etherscan_api_key

# Optional: Gas price settings
GAS_PRICE=20000000000  # 20 gwei
GAS_LIMIT=8000000
#-------------------------------------- CONTRACT DEPLOYMENT ----------------------------------


#-------------------------------------- NUVEX -------------------------------------------------

SRC_CHAIN_RPC=https://eth.merkle.io
DST_CHAIN_RPC=wss://bsc-rpc.publicnode.com
SRC_CHAIN_CREATE_FORK=true
DST_CHAIN_CREATE_FORK=true

# Bridge Service Configuration
PORT=3000
XRPL_NETWORK=wss://s.altnet.rippletest.net:51233
CORS_ORIGIN=*
LOG_LEVEL=info
NODE_ENV=development

# GitHub Token for private packages
GITHUB_TOKEN=your_github_token_here


#-------------------------------------- NUVEX -------------------------------------------------

#-------------------------------------- Default Config ----------------------------------------
SRC_CHAIN_RPC=https://eth.merkle.io
DST_CHAIN_RPC=wss://bsc-rpc.publicnode.com
